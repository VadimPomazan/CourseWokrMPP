<h1>Your Family Members</h1>

<%= link_to 'Add Family Member', new_family_member_path %>
<%= button_to 'Add Yourself', add_self_family_members_path(current_user.id), method: :post %>

<ul>
  <% @family_members.each do |family_member| %>
    <li>
      <%= family_member.family_member_name %> - <%= family_member.person.full_name %>
    </li>
  <% end %>
</ul>
